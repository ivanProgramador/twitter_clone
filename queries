use twitter_clone;

CREATE TABLE tweets(
    
  id int not null PRIMARY KEY AUTO_INCREMENT,
  id_usuario int not null,
  tweet varchar(140) not null,
  data datetime default CURRENT_TIMESTAMP
    
);


use twitter_clone;

create table usuarios_seguidores(
	id int not null primary key auto_increment,
	id_usuario int not null,
	id_usuario_seguindo int not null
);